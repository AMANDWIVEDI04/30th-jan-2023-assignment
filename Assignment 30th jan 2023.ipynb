    "# Answer 3 \n",
    "# Define dictionary mapping city names to monuments \n",
    "city_monuments = {\n",
    "     \"Delhi\" : [ \" Red fort \"],\n",
    "     \" Agra \": [ \" taj mahal\"],\n",
    "     \" Jaipur\": [\" Jai mahal \"]\n",
    "}\n",
    "\n",
    "# prompt user for city name \n",
    "  city _ name = input (\" enter a city name : \")\n",
    "    \n",
    "# check if city name is in dictionary \n",
    "if city _ name in city _ monuments:\n",
    "    # if city name is in dictionary , display monuments \n",
    "    monuments = city_monuments[city_name]\n",
    "    print(\"monuments in \" , city_name, \":\")\n",
    "    for monument in monuments :\n",
    "        print(\" - \" + monument)\n",
    "else : \n",
    "    # if city name is not in dictionnary , display error message \n",
    "    print (\"No monuments found for \", city_name)\n"
